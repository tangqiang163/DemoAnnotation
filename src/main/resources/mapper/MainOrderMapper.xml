      car_order_item.update_by AS c_update_by
  FROM
      main_order,
      sub_order,
      car_order_item
  WHERE
      main_order.main_order_no = sub_order.main_order_no
      AND sub_order.sub_order_no = car_order_item.sub_order_no
      AND main_order.is_deleted = 0
      AND sub_order.is_deleted = 0
      AND car_order_item.is_deleted = 0
      AND main_order.main_order_no = #{mainOrderNo}
